{
  "name": "gps-track-maps",
  "version": "1.0.0",
  "description": "Showcase of web pages visualizing GPS track log, by using OpenLayers, leaflets, google and bing maps",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/*.js'",
    "start": "parcel --no-source-maps 'src/*.html'",
    "prebuild": "rm -f dist/*.*.*",
    "build": "parcel build --no-source-maps --public-url . 'src/*.html'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anineco/gps-track-maps.git"
  },
  "author": "Nyanta Anineco <anineco@nifty.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anineco/gps-track-maps/issues"
  },
  "homepage": "https://github.com/anineco/gps-track-maps#readme",
  "dependencies": {
    "ol": "^7.2.2",
    "ol-layerswitcher": "^4.1.0",
    "ol-mapbox-style": "^9.5.0",
    "ol-popup": "^5.0.0"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "is-svg": ">=4.3.2",
    "parcel": "^2.8.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
